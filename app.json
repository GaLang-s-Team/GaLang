{
  "expo": {
    "name": "GaLang",
    "slug": "galang",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E4E7C9"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wildhanhidayatullah.GaLang"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f7e7df83-602c-4634-87e4-5d4314feea38"
      },
      "router": {
        "origin": false
      }
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "owner": "wildhanhidayatullah"
  }
}
